            PLATEFORM="ALL"
            RELEASE_DATE="20160301"
            SERIAL=""
            STARTABLE="true"
            STATUS="ALPHA"
            VERSION="1.2">
        <SIGNATURE/>
    </HEADER>

    <FAMILIES>
        <FAMILY>Databases/Neo4j</FAMILY>
        <FAMILY>Big Data/Neo4j</FAMILY>
    </FAMILIES>

    <DOCUMENTATION>
        <URL/>
    </DOCUMENTATION>

    <CONNECTORS>
        <CONNECTOR CTYPE="FLOW"
                   MAX_INPUT="1"
                   MAX_OUTPUT="0"
                   MIN_INPUT="0"
                   MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="ITERATE"
                   MAX_OUTPUT="0"
                   MAX_INPUT="1" />
        <CONNECTOR CTYPE="SUBJOB_OK"
                   MAX_INPUT="1" />
        <CONNECTOR CTYPE="SUBJOB_ERROR"
                   MAX_INPUT="1" />
        <CONNECTOR CTYPE="COMPONENT_OK"
                   MAX_INPUT="0"
                   MAX_OUTPUT="1"
                   MIN_INPUT="0"
                   MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="COMPONENT_ERROR"
                   MAX_INPUT="0"
                   MAX_OUTPUT="1"
                   MIN_INPUT="0"
                   MIN_OUTPUT="0" />
        <CONNECTOR CTYPE="RUN_IF" />
    </CONNECTORS>

    <PARAMETERS>
    	<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="10" GROUP="CONNECTION_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

        <PARAMETER NAME="CONNECTION"
                   FIELD="COMPONENT_LIST"
                   REQUIRED="true"
                   FILTER="tNeo4jConnection"
                   NUM_ROW="20"
                   SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
                   GROUP="CONNECTION_GROUP"/>
                   
		<PARAMETER NAME="DATABASE_PATH" FIELD="DIRECTORY"	REPOSITORY_VALUE="DATABASE_PATH" REQUIRED="true" NUM_ROW="25"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SHUTDOWN_DB" FIELD="CHECK" NUM_ROW="26"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" GROUP="CONNECTION_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

        <PARAMETER NAME="LABEL_FIELD"
                   FIELD="COLUMN_LIST"
                   NUM_ROW="30"
                   GROUP="SCHEMA_GROUP">
            <DEFAULT>"label"</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="IMPORT_INDEX_NAME"
                   FIELD="TEXT"
                   REPOSITORY_VALUE="NEO4J_BATCH_INDEXNAME"
                   REQUIRED="true"
                   NUM_ROW="40"
                   GROUP="IMPORT_INDEX_GROUP" >
            <DEFAULT>"ALL_NODE"</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="IMPORT_ID_FIELD"
                   FIELD="COLUMN_LIST"
                   REQUIRED="true"
                   NUM_ROW="50"
                   GROUP="IMPORT_INDEX_GROUP" />

        <PARAMETER
                NAME="INSERT_IMPORT_ID_FIELD"
                FIELD="CHECK"
                REQUIRED="true"
                NUM_ROW="60"
                GROUP="IMPORT_INDEX_GROUP" >
            <DEFAULT>true</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="IMPORT_INDEX_CACHE_SIZE"
                   FIELD="INTEGER"
                   NUM_ROW="70"
                   GROUP="IMPORT_INDEX_GROUP">
            <DEFAULT>0</DEFAULT>
        </PARAMETER>

    </PARAMETERS>

	<ADVANCED_PARAMETERS>
        <PARAMETER NAME="CONFIGURATION"
                   FIELD="TABLE"
                   GROUP="FINE_TUNING_GROUP"
                   NUM_ROW="10"
                   NB_LINES="10"
                   SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
            <ITEMS>
                <ITEM NAME="KEY" VALUE="dbms.pagecache.memory"/>
                <ITEM NAME="VALUE" VALUE="2G"/>
            </ITEMS>

        </PARAMETER>

    </ADVANCED_PARAMETERS>

