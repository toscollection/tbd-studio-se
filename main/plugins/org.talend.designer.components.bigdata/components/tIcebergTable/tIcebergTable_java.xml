<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20110808A"
    STARTABLE="true"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Big Data/Iceberg</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR NAME="PARTITIONS" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" FILTER="tIcebergConnection" NUM_ROW="1" />

    <PARAMETER NAME="TABLE" FIELD="TEXT" REQUIRED="true" NUM_ROW="2">
        <DEFAULT>"myTable"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="TABLE_ACTION" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="3">
        <ITEMS DEFAULT="CREATE_TABLE">
            <ITEM NAME="ALTER_TABLE" VALUE="ALTER_TABLE" />
            <ITEM NAME="CREATE_TABLE" VALUE="CREATE_TABLE" />
            <ITEM NAME="CREATE_TABLE_IF_NOT_EXISTS" VALUE="CREATE_TABLE_IF_NOT_EXISTS" />
            <ITEM NAME="DROP_TABLE" VALUE="DROP_TABLE" />
            <ITEM NAME="DROP_TABLE_IF_EXISTS" VALUE="DROP_TABLE_IF_EXISTS" />
            <ITEM NAME="TRUNCATE" VALUE="TRUNCATE" />
        </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="ALTER_TABLE_ACTION" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="3" SHOW_IF="TABLE_ACTION=='ALTER_TABLE'">
        <ITEMS DEFAULT="SET_PARTITIONS">
            <ITEM NAME="SET_PARTITIONS" VALUE="SET_PARTITIONS" />
        </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="NEW_PARTITIONS" FIELD="TABLE" NUM_ROW="4" SHOW_IF="(isShow[ALTER_TABLE_ACTION]) AND (ALTER_TABLE_ACTION=='SET_PARTITIONS')">
    	<ITEMS>
    		<ITEM NAME="PARTITION" />
    	</ITEMS>
    </PARAMETER>

    <PARAMETER NAME="CREATE_LIKE" FIELD="CHECK" SHOW_IF="((TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')) AND (CREATE_AS_SELECT=='false')" NUM_ROW="3">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="CREATE_AS_SELECT" FIELD="CHECK" SHOW_IF="((TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')) AND (CREATE_LIKE=='false')" NUM_ROW="3">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="PURGE" FIELD="CHECK" SHOW_IF="((TABLE_ACTION=='DROP_TABLE') OR (TABLE_ACTION=='DROP_TABLE_IF_EXISTS'))" NUM_ROW="3">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="4" SHOW_IF="((TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')) AND (CREATE_LIKE=='false') AND (CREATE_AS_SELECT=='false')"/>

    <PARAMETER NAME="SOURCE_TABLE" FIELD="TEXT" NUM_ROW="4" SHOW_IF="(isShow[CREATE_LIKE]) AND (CREATE_LIKE=='true')">
        <DEFAULT>"sourceTable"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="AS_SELECT_QUERY" FIELD="MEMO" NUM_ROW="4" SHOW_IF="(isShow[CREATE_AS_SELECT]) AND (CREATE_AS_SELECT=='true')">
        <DEFAULT>"SELECT * FROM sourceTable"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SET_FORMAT" FIELD="CHECK" NUM_ROW="5" SHOW_IF="(TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="FORMAT" FIELD="CLOSED_LIST" NUM_ROW="5" SHOW_IF="(TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')">
        <ITEMS DEFAULT="PARQUET">
            <ITEM NAME="AVRO" VALUE="AVRO" />
            <ITEM NAME="ORC" VALUE="ORC" />
            <ITEM NAME="PARQUET" VALUE="PARQUET" />
        </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="EXTERNAL" FIELD="CHECK" NUM_ROW="6" SHOW_IF="(TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SET_PARTITIONS" FIELD="CHECK" NUM_ROW="7" SHOW_IF="((TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')) AND ((!isShow[CREATE_AS_SELECT]) OR (CREATE_AS_SELECT=='false'))">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="PARTITIONS" FIELD="SCHEMA_TYPE" NUM_ROW="7" CONTEXT="PARTITIONS" SHOW_IF="(isShow[SET_PARTITIONS]) AND (SET_PARTITIONS=='true')"></PARAMETER>

    <PARAMETER NAME="SET_LOCATION" FIELD="CHECK" NUM_ROW="8" SHOW_IF="(TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="LOCATION" FIELD="TEXT" NUM_ROW="8" SHOW_IF="(isShow[SET_LOCATION]) AND (SET_LOCATION=='true')">
        <DEFAULT>"scheme://location/path"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="TABLE_PROPERTIES" FIELD="TABLE" NUM_ROW="9" NB_LINES="3" SHOW_IF="(TABLE_ACTION=='CREATE_TABLE') OR (TABLE_ACTION=='CREATE_TABLE_IF_NOT_EXISTS')">
        <ITEMS>
            <ITEM NAME="PROPERTY" />
            <ITEM NAME="VALUE" />
        </ITEMS>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>
  <RETURNS/>
</COMPONENT>
