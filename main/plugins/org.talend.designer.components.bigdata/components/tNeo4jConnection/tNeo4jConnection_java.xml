	<HEADER AUTHOR="Antoine Rouaze" COMPATIBILITY="ALL"
		DATA_AUTO_PROPAGATE="false" PLATEFORM="ALL" RELEASE_DATE="20080229A"
		SERIAL="" STARTABLE="true" STATUS="ALPHA" VERSION="0.102">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>Databases NoSQL/Neo4j (Deprecated)</FAMILY>
		<FAMILY>Big Data/Neo4j (Deprecated)</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1"
			MIN_INPUT="0" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1"
			MIN_INPUT="0" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" MAX_OUTPUT="1"
			MIN_INPUT="0" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="0" MAX_OUTPUT="1"
			MIN_INPUT="0" MIN_OUTPUT="0" />
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"
			MIN_INPUT="0" MIN_OUTPUT="0" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" NUM_ROW="1"
			REPOSITORY_VALUE="NOSQL:NEO4J" />

		<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST"
			REPOSITORY_VALUE="DB_VERSION" NUM_ROW="2" GROUP="CONNECTION_GROUP">
			<ITEMS DEFAULT="NEO4J_3_2_X">
				<ITEM NAME="NEO4J_3_2_X" VALUE="NEO4J_3_2_X" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="REMOTE_SERVER" FIELD="CHECK" REPOSITORY_VALUE="REMOTE_SERVER" NUM_ROW="6" GROUP="CONNECTION_GROUP">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="DATABASE_PATH" FIELD="DIRECTORY"	REPOSITORY_VALUE="DATABASE_PATH" REQUIRED="true" NUM_ROW="10"
			SHOW_IF="(REMOTE_SERVER == 'false')" GROUP="CONNECTION_GROUP">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER 	NAME="SERVER_URL" FIELD="TEXT" REPOSITORY_VALUE="SERVER_URL"	REQUIRED="true" NUM_ROW="10" 
					SHOW_IF="(REMOTE_SERVER == 'true')" GROUP="CONNECTION_GROUP">
			<DEFAULT>"bolt://localhost:7687"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="READ_ONLY_MODE" FIELD="CHECK" NUM_ROW="30"
			SHOW_IF="(DB_VERSION != 'NEO4J_3_2_X') AND (REMOTE_SERVER == 'false')" GROUP="CONNECTION_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

        <!--  username only on remote server, but mandatory for neo4j 2.2.x -->
		<PARAMETER NAME="SET_USERNAME" FIELD="CHECK" NUM_ROW="40"	GROUP="CONNECTION_GROUP" REPOSITORY_VALUE="SET_USERNAME"
			SHOW_IF="(REMOTE_SERVER == 'true') AND ((DB_VERSION == 'NEO4J_1_X_X') OR (DB_VERSION == 'NEO4J_2_1_X'))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="50" GROUP="CONNECTION_GROUP" REPOSITORY_VALUE="USERNAME"
			SHOW_IF="(REMOTE_SERVER == 'true') AND ((SET_USERNAME == 'true') OR ((DB_VERSION != 'NEO4J_1_X_X') AND (DB_VERSION != 'NEO4J_2_1_X')))">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="true"
			NUM_ROW="50" GROUP="CONNECTION_GROUP" REPOSITORY_VALUE="PASSWORD"
			SHOW_IF="(REMOTE_SERVER == 'true') AND ((SET_USERNAME == 'true') OR ((DB_VERSION != 'NEO4J_1_X_X') AND (DB_VERSION != 'NEO4J_2_1_X')))">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>


	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
        <PARAMETER NAME="CONFIGURATION"
                   FIELD="TABLE"
                   GROUP="FINE_TUNING_GROUP"
                   NUM_ROW="10"
                   NB_LINES="10"
                   SHOW_IF="REMOTE_SERVER == 'false'">
            <ITEMS>
                <ITEM NAME="KEY" VALUE="dbms.pagecache.memory"/>
                <ITEM NAME="VALUE" VALUE="2G"/>
            </ITEMS>

        </PARAMETER>

    </ADVANCED_PARAMETERS>
    
