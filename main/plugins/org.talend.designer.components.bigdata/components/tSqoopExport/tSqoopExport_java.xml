				<ITEM NAME="ADDITIONAL_ARGUMENT" FIELD="TEXT" />
				<ITEM NAME="ADDITIONAL_VALUE" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="ADDITIONAL_JAVA" FIELD="TABLE" NUM_ROW="30"
			NB_LINES="5" SHOW_IF="USE_COMMANDLINE=='false'">
			<ITEMS>
				<ITEM NAME="ADDITIONAL_ARGUMENT" FIELD="TEXT" />
				<ITEM NAME="ADDITIONAL_VALUE" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="USE_SPEED_PARALLEL" FIELD="CHECK" NUM_ROW="32"
			SHOW_IF="(USE_COMMANDLINE=='true')" GROUP="SPEED_PARALLEL">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="NOTE_CUSTOMER" FIELD="LABEL" REQUIRED="true"
			NUM_ROW="32" COLOR="255;0;0" SHOW_IF="(USE_COMMANDLINE=='true')"
			GROUP="SPEED_PARALLEL">
			<DEFAULT>This function only applies for HDP and Teradata</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SPECIFIC_PARAMS" FIELD="TABLE" NUM_ROW="35"
			NB_LINES="5" SHOW_IF="(USE_COMMANDLINE=='true') AND (USE_SPEED_PARALLEL=='true')"
			GROUP="SPEED_PARALLEL">
			<ITEMS>
				<ITEM NAME="SPECIFIC_PARAM" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="teradata-db-output-batch-size">
						<ITEM NAME="teradata-db-output-batch-size" VALUE="teradata.db.output.batch.size" />
						<ITEM NAME="teradata-db-output-fastload-number-max" VALUE="teradata.db.output.fastload.number.max" />
						<ITEM NAME="teradata-db-output-fastload-socket-host" VALUE="teradata.db.output.fastload.socket.host" />
						<ITEM NAME="teradata-db-output-fastload-socket-port" VALUE="teradata.db.output.fastload.socket.port" />
						<ITEM NAME="teradata-db-output-file-format" VALUE="teradata.db.output.file.format" />
						<ITEM NAME="teradata-db-output-job-type" VALUE="teradata.db.output.job.type" />
						<ITEM NAME="teradata-db-output-method" VALUE="teradata.db.output.method" />
						<ITEM NAME="teradata-db-output-num-mappers" VALUE="teradata.db.output.num.mappers" />
						<ITEM NAME="teradata-db-output-num-partitions-in-staging"
