	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="NOTE_COMMANDINE" FIELD="LABEL" REQUIRED="true"
			NUM_ROW="1" SHOW_IF="(USE_COMMANDLINE=='true')">
			<DEFAULT>The available options are based on Sqoop 1.4.4. If you use
				an earlier version, read the Sqoop documentation in order to find
				those which are not available.
			</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_COMMANDLINE" FIELD="RADIO" REQUIRED="true"
			NUM_ROW="2" GROUP="MODE">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_JAVAAPI" FIELD="RADIO" REQUIRED="true"
			NUM_ROW="3" GROUP="MODE">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="HADOOP_PROPERTY" FIELD="PROPERTY_TYPE"
			REPOSITORY_VALUE="HADOOPCLUSTER|HDFS" NUM_ROW="4" SHOW_IF="USE_JAVAAPI=='true'"></PARAMETER>

		<PARAMETER NAME="SQOOP" FIELD="HADOOP_DISTRIBUTION" NUM_ROW="5"
			SHOW_IF="(USE_JAVAAPI=='true')" GROUP="VERSION" />

		<PARAMETER NAME="HADOOP_CUSTOM_JARS" GROUP="VERSION"
			FIELD="HADOOP_JARS_DIALOG" NUM_ROW="5"
			SHOW_IF="(USE_JAVAAPI=='true') AND (DISTRIBUTION=='CUSTOM')"
			REPOSITORY_VALUE="HADOOP_PROPERTY/HADOOP_CUSTOM_JARS">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="USE_YARN" FIELD="CHECK" NUM_ROW="6"
			SHOW_IF="(USE_JAVAAPI=='true') AND (DISTRIBUTION=='CUSTOM')" GROUP="VERSION"
			REPOSITORY_VALUE="HADOOP_PROPERTY/USE_YARN">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CROSS_PLATFORM_SUBMISSION" GROUP="VERSION"
			FIELD="CHECK" NUM_ROW="6"
			SHOW_IF="(USE_JAVAAPI=='true') AND (DISTRIBUTION=='CUSTOM') AND (USE_YARN=='true')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FS_DEFAULT_NAME" FIELD="TEXT" NUM_ROW="7"
			REQUIRED="true" SHOW_IF="USE_JAVAAPI == 'true'" GROUP="CONFIGURATION"
			REPOSITORY_VALUE="HADOOP_PROPERTY/FS_DEFAULT_NAME">
			<DEFAULT>"hdfs://localhost:8020"</DEFAULT>
			<DEFAULT IF="DISTRIBUTION == 'MAPR'">"maprfs:///"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="MAPRED_JOB_TRACKER" FIELD="TEXT" NUM_ROW="8"
			REQUIRED="true"
			SHOW_IF="(USE_JAVAAPI == 'true') AND ((DISTRIBUTION=='CUSTOM' AND USE_YARN=='false') OR (DISTRIBUTION!='CUSTOM' AND DISTRIB[DISTRIBUTION, DB_VERSION].isHadoop1[]))"
			GROUP="CONFIGURATION" REPOSITORY_VALUE="HADOOP_PROPERTY/MAPRED_JOB_TRACKER">
			<DEFAULT>"localhost:8021"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="RESOURCE_MANAGER" FIELD="TEXT" NUM_ROW="8"
			REQUIRED="true"
			SHOW_IF="(USE_JAVAAPI == 'true') AND ((DISTRIBUTION=='CUSTOM' AND USE_YARN=='true') OR (DISTRIBUTION!='CUSTOM' AND DISTRIB[DISTRIBUTION, DB_VERSION].isHadoop2[]))"
			GROUP="CONFIGURATION" REPOSITORY_VALUE="HADOOP_PROPERTY/MAPRED_RESOURCE_MANAGER">
			<DEFAULT>"localhost:8032"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SET_SCHEDULER_ADDRESS" FIELD="CHECK"
			NUM_ROW="20" REQUIRED="true" SHOW_IF="isShow[RESOURCE_MANAGER]"
			GROUP="CONFIGURATION">
			<DEFAULT IF="(PROPERTY == 'REPOSITORY')">true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="RESOURCEMANAGER_SCHEDULER_ADDRESS" FIELD="TEXT"
			REPOSITORY_VALUE="HADOOP_PROPERTY/RESOURCEMANAGER_SCHEDULER_ADDRESS" NUM_ROW="20"
			REQUIRED="true"
			SHOW_IF="(isShow[SET_SCHEDULER_ADDRESS]) AND (SET_SCHEDULER_ADDRESS=='true')"
			GROUP="CONFIGURATION">
			<DEFAULT>"localhost:8030"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SET_JOBHISTORY_ADDRESS" FIELD="CHECK"
			NUM_ROW="21" REQUIRED="true" SHOW_IF="isShow[RESOURCE_MANAGER]"
			GROUP="CONFIGURATION">
			<DEFAULT IF="(PROPERTY == 'REPOSITORY')">true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="JOBHISTORY_ADDRESS" FIELD="TEXT" NUM_ROW="21"
			REPOSITORY_VALUE="HADOOP_PROPERTY/JOBHISTORY_ADDRESS" REQUIRED="true"
			SHOW_IF="(isShow[SET_JOBHISTORY_ADDRESS]) AND (SET_JOBHISTORY_ADDRESS=='true')"
			GROUP="CONFIGURATION">
			<DEFAULT>"localhost:10020"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SET_STAGING_DIRECTORY" FIELD="CHECK"
			NUM_ROW="22" REQUIRED="true" SHOW_IF="isShow[RESOURCE_MANAGER]"
			GROUP="CONFIGURATION">
			<DEFAULT IF="(PROPERTY == 'REPOSITORY')">true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="STAGING_DIRECTORY" FIELD="TEXT" NUM_ROW="22"
			REPOSITORY_VALUE="HADOOP_PROPERTY/STAGING_DIRECTORY" REQUIRED="true"
			SHOW_IF="(isShow[SET_STAGING_DIRECTORY]) AND (SET_STAGING_DIRECTORY=='true')"
			GROUP="CONFIGURATION">
			<DEFAULT>"/user"</DEFAULT>
		</PARAMETER>


		<PARAMETER NAME="USE_DATANODE_HOSTNAME" FIELD="CHECK"
			REPOSITORY_VALUE="USE_DATANODE_HOSTNAME" NUM_ROW="23" REQUIRED="true"
			SHOW_IF="(USE_JAVAAPI == 'true') AND ((DISTRIBUTION=='CUSTOM') OR (DISTRIB[DISTRIBUTION, DB_VERSION].doSupportUseDatanodeHostname[]))"
			GROUP="CONFIGURATION">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_KRB" FIELD="CHECK" NUM_ROW="25"
			SHOW_IF="USE_JAVAAPI == 'true' AND ((DISTRIBUTION!='CUSTOM' AND DISTRIB[DISTRIBUTION, DB_VERSION].doSupportKerberos[]) OR (DISTRIBUTION=='CUSTOM'))"
			GROUP="AUTHENTICATION" REPOSITORY_VALUE="HADOOP_PROPERTY/USE_KRB">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NAMENODE_PRINCIPAL" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="26" SHOW_IF="isShow[USE_KRB] AND (USE_KRB=='true')" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="HADOOP_PROPERTY/NAMENODE_PRINCIPAL">
			<DEFAULT>"nn/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="JOBTRACKER_PRINCIPAL" FIELD="TEXT"
			REQUIRED="true" NUM_ROW="27"
			SHOW_IF="isShow[USE_KRB] AND (USE_KRB=='true') AND isShow[MAPRED_JOB_TRACKER]"
			GROUP="AUTHENTICATION" REPOSITORY_VALUE="HADOOP_PROPERTY/JOBTRACKER_PRINCIPAL">
			<DEFAULT>"jt/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="RESOURCEMANAGER_PRINCIPAL" FIELD="TEXT"
			REQUIRED="true" NUM_ROW="27"
			SHOW_IF="isShow[USE_KRB] AND (USE_KRB=='true') AND isShow[RESOURCE_MANAGER]"
			GROUP="AUTHENTICATION" REPOSITORY_VALUE="HADOOP_PROPERTY/RESOURCEMANAGER_PRINCIPAL">
			<DEFAULT>"yarn/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="JOBHISTORY_PRINCIPAL" FIELD="TEXT"
			REQUIRED="true" NUM_ROW="28"
			SHOW_IF="isShow[USE_KRB] AND (USE_KRB=='true') AND isShow[RESOURCE_MANAGER]"
			GROUP="AUTHENTICATION" REPOSITORY_VALUE="HADOOP_PROPERTY/JOBHISTORY_PRINCIPAL">
			<DEFAULT>"mapred/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_KEYTAB" FIELD="CHECK" NUM_ROW="29"
			SHOW_IF="isShow[USE_KRB] AND (USE_KRB=='true')" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="HADOOP_PROPERTY/USE_KEYTAB">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PRINCIPAL" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30" SHOW_IF="isShow[USE_KEYTAB] AND (USE_KEYTAB=='true')"
			GROUP="AUTHENTICATION" REPOSITORY_VALUE="HADOOP_PROPERTY/KEYTAB_PRINCIPAL">
			<DEFAULT>"hdfs"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="KEYTAB_PATH" FIELD="FILE" REQUIRED="true"
			NUM_ROW="30" SHOW_IF="isShow[USE_KEYTAB] AND (USE_KEYTAB=='true')"
			GROUP="AUTHENTICATION" REPOSITORY_VALUE="HADOOP_PROPERTY/KEYTAB_PATH">
			<DEFAULT>"/tmp/hdfs.headless.keytab"</DEFAULT>
		</PARAMETER>

       <PARAMETER NAME="USE_MAPRTICKET" FIELD="CHECK" NUM_ROW="31"
            REPOSITORY_VALUE="USE_MAPRTICKET" GROUP="AUTHENTICATION_MAPR" 
            SHOW_IF="USE_JAVAAPI == 'true' AND (DISTRIB[DISTRIBUTION, DB_VERSION].doSupportMapRTicket[])">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>


        <PARAMETER NAME="MAPRTICKET_USERNAME" FIELD="TEXT" GROUP="AUTHENTICATION_MAPR"
            REPOSITORY_VALUE="MAPRTICKET_USERNAME" REQUIRED="true" NUM_ROW="32"
            SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true')) AND (((isShow[USE_KRB]) AND (USE_KRB=='false')) OR (!isShow[USE_KRB])) ">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="MAPRTICKET_PASSWORD" FIELD="PASSWORD" GROUP="AUTHENTICATION_MAPR"
            REPOSITORY_VALUE="MAPRTICKET_PASSWORD" REQUIRED="true" NUM_ROW="32"
            SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true')) AND (((isShow[USE_KRB]) AND (USE_KRB=='false')) OR (!isShow[USE_KRB])) ">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="MAPRTICKET_CLUSTER" FIELD="TEXT" GROUP="AUTHENTICATION_MAPR"
            REPOSITORY_VALUE="MAPRTICKET_CLUSTER" REQUIRED="true" NUM_ROW="33"
            SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
            <DEFAULT>"demo.mapr.com"</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="MAPRTICKET_DURATION" FIELD="TEXT" GROUP="AUTHENTICATION_MAPR"
            REPOSITORY_VALUE="MAPRTICKET_DURATION" REQUIRED="true" NUM_ROW="34"
            SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
            <DEFAULT>86400L</DEFAULT>
        </PARAMETER>


		<PARAMETER NAME="HADOOP_USER" FIELD="TEXT" GROUP="AUTHENTICATION"
			REQUIRED="true" NUM_ROW="37"
			SHOW_IF="USE_JAVAAPI == 'true' AND (((DISTRIBUTION!='CUSTOM' AND DISTRIB[DISTRIBUTION, DB_VERSION].doSupportImpersonation[]) OR (DISTRIBUTION=='CUSTOM')))
			AND (!DISTRIB[DISTRIBUTION, DB_VERSION].doSupportKerberos[] OR USE_KRB == 'false')"
			REPOSITORY_VALUE="HADOOP_PROPERTY/USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="JDBC_PROPERTY" FIELD="PROPERTY_TYPE"
			REPOSITORY_VALUE="DATABASE:JDBC" NUM_ROW="39"></PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="TEXT" NUM_ROW="40"
			GROUP="COMMON_SQOOP_OPTIONS" REPOSITORY_VALUE="JDBC_PROPERTY/URL">
			<DEFAULT>"jdbc:mysql://localhost/database_name"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USERNAME" FIELD="TEXT" NUM_ROW="42"
			GROUP="COMMON_SQOOP_OPTIONS" REPOSITORY_VALUE="JDBC_PROPERTY/USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD_STORED_IN_FILE" FIELD="CHECK"
			NUM_ROW="43" GROUP="COMMON_SQOOP_OPTIONS"
			SHOW_IF="(USE_COMMANDLINE == 'true' OR (USE_JAVAAPI=='true' AND (DISTRIBUTION=='CUSTOM' OR DISTRIB[DISTRIBUTION, DB_VERSION].doJavaAPISupportStorePasswordInFile[])))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD_FILE" FIELD="TEXT" NUM_ROW="44"
			GROUP="COMMON_SQOOP_OPTIONS"
			SHOW_IF="(PASSWORD_STORED_IN_FILE=='true') AND (isShow[PASSWORD_STORED_IN_FILE])">
			<DEFAULT>"file:///"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" NUM_ROW="44"
			GROUP="COMMON_SQOOP_OPTIONS" REPOSITORY_VALUE="JDBC_PROPERTY/PASSWORD"
			SHOW_IF="PASSWORD_STORED_IN_FILE=='false' OR (USE_JAVAAPI=='true' AND (DISTRIBUTION!='CUSTOM' AND !DISTRIB[DISTRIBUTION, DB_VERSION].doJavaAPISupportStorePasswordInFile[]))">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DRIVER_JAR" FIELD="TABLE" NUM_ROW="46"
			NB_LINES="2" REPOSITORY_VALUE="JDBC_PROPERTY/DRIVER_JAR" REQUIRED="false"
			GROUP="COMMON_SQOOP_OPTIONS" SHOW_IF="USE_JAVAAPI=='true'">
			<ITEMS>
				<ITEM NAME="JAR_NAME" FIELD="MODULE_LIST" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="DRIVER_CLASS" FIELD="TEXT" NUM_ROW="47"
			REPOSITORY_VALUE="JDBC_PROPERTY/DRIVER_CLASS" REQUIRED="true"
			GROUP="COMMON_SQOOP_OPTIONS" SHOW_IF="USE_JAVAAPI=='true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="TEXT" NUM_ROW="48"
			GROUP="CONTROL_SQOOP_OPTIONS" SHOW_IF="CALL_PROC_STOC=='false'">
			<DEFAULT>"table_name"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SOURCE" FIELD="CLOSED_LIST" NUM_ROW="49" GROUP="CONTROL_SQOOP_OPTIONS">
			<ITEMS DEFAULT="HDFS">
				<ITEM NAME="HDFS" VALUE="HDFS" />
				<ITEM NAME="HCAT" VALUE="HCAT" SHOW_IF="USE_COMMANDLINE=='true'"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="EXPORT_DIR" FIELD="TEXT" NUM_ROW="50" SHOW_IF="SOURCE == 'HDFS'"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="HCAT_DATABASE" FIELD="TEXT" NUM_ROW="51" SHOW_IF="SOURCE == 'HCAT'"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>"default"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="HCAT_TABLE" FIELD="TEXT" NUM_ROW="51" SHOW_IF="SOURCE == 'HCAT'"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIRECT" FIELD="CHECK" NUM_ROW="53"
			GROUP="CONTROL_SQOOP_OPTIONS"
			SHOW_IF="(DEFINE_UPDATE_MODE=='false' OR (DEFINE_UPDATE_MODE=='true' AND UPDATE_MODE=='updateonly'))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_MAPPERS" FIELD="CHECK" NUM_ROW="54"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPPERS" FIELD="TEXT" NUM_ROW="54"
			SHOW_IF="USE_MAPPERS=='true'" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>"2"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CALL_PROC_STOC" FIELD="CHECK" NUM_ROW="55"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PROC_STOC" FIELD="TEXT" NUM_ROW="55"
			SHOW_IF="CALL_PROC_STOC=='true'" GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BATCH_MODE" FIELD="CHECK" NUM_ROW="58"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CLEAR_STAGING_TABLE" FIELD="CHECK" NUM_ROW="59"
			GROUP="CONTROL_SQOOP_OPTIONS" SHOW_IF="(USE_COLUMN_FOR_UPDATE=='false' AND CALL_PROC_STOC=='false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DEFINE_STAGING_TABLE" FIELD="CHECK"
			NUM_ROW="60" GROUP="CONTROL_SQOOP_OPTIONS"
			SHOW_IF="(USE_COLUMN_FOR_UPDATE=='false' AND CALL_PROC_STOC=='false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="STAGING_TABLE" FIELD="TEXT" NUM_ROW="60"
			SHOW_IF="(USE_COLUMN_FOR_UPDATE=='false' AND CALL_PROC_STOC=='false' AND DEFINE_STAGING_TABLE=='true')"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DEFINE_UPDATE_MODE" FIELD="CHECK" NUM_ROW="62"
			GROUP="CONTROL_SQOOP_OPTIONS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="UPDATE_MODE" FIELD="CLOSED_LIST" NUM_ROW="62"
			SHOW_IF="DEFINE_UPDATE_MODE=='true'" GROUP="CONTROL_SQOOP_OPTIONS">
			<ITEMS DEFAULT="updateonly">
				<ITEM NAME="updateonly" VALUE="updateonly" />
				<ITEM NAME="allowinsert" VALUE="allowinsert" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="USE_COLUMN_FOR_UPDATE" FIELD="CHECK"
			NUM_ROW="65" GROUP="CONTROL_SQOOP_OPTIONS" SHOW_IF="CALL_PROC_STOC=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="COLUMN_FOR_UPDATE" FIELD="TABLE" NUM_ROW="66"
			GROUP="CONTROL_SQOOP_OPTIONS" NB_LINES="5"
			SHOW_IF="(USE_COLUMN_FOR_UPDATE=='true' AND CALL_PROC_STOC=='false')">
			<ITEMS>
				<ITEM NAME="COLUMN_NAME" FIELD="TEXT" VALUE="&#34;&#34;" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="PRINT_LOG" FIELD="CHECK" NUM_ROW="100">
			<DEFAULT IF="USE_COMMANDLINE=='true'">true</DEFAULT>
			<DEFAULT IF="USE_COMMANDLINE=='false'">false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="VERBOSE" FIELD="CHECK" NUM_ROW="100"
			SHOW_IF="PRINT_LOG=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="110">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="NOTE_MULTIPLE_SQOOP" FIELD="LABEL"
			REQUIRED="true" NUM_ROW="120">
			<DEFAULT>Note: If you need to chain several sqoop export using same
				jdbc driver, please run them in separate subjobs via tRunJob (due to
				a sqoop cache issue).
			</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="MYSQL_DELIMITERS" FIELD="CHECK" NUM_ROW="10"
			SHOW_IF="USE_COMMANDLINE=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DEFINE_JAVA_MAPPING" FIELD="CHECK" NUM_ROW="12"
			GROUP="TYPE_MAPPING">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="JAVA_TYPE_MAPPING" FIELD="TABLE" NUM_ROW="13"
			GROUP="TYPE_MAPPING" NB_LINES="5" NOT_SHOW_IF="(DEFINE_JAVA_MAPPING=='false')">
			<ITEMS>
				<ITEM NAME="COLUMN_NAME" FIELD="TEXT" VALUE="&#34;&#34;" />
				<ITEM NAME="JAVA_TYPE" FIELD="TEXT" VALUE="&#34;&#34;" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="ADDITIONAL_ARGUMENTS" FIELD="TABLE"
			NUM_ROW="15" NB_LINES="5" SHOW_IF="USE_COMMANDLINE=='true'" GROUP="ADDITIONAL">
