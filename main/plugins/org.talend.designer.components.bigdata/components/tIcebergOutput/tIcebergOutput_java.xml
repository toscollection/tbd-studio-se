<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070210A"
		STARTABLE="true" PARTITIONING="NONE" LOG4J_ENABLED="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>
	<FAMILIES>
		<FAMILY>Big Data/Iceberg</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" FILTER="tIcebergConnection" NUM_ROW="10"/>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="11" />
		<PARAMETER NAME="TABLE" FIELD="TEXT" REQUIRED="true" NUM_ROW="12">
			<DEFAULT>"myTable"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_BATCH" FIELD="CHECK" NUM_ROW="13" SHOW_IF="(USE_INSERT_OVERWRITE=='false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="BATCH_SIZE" FIELD="TEXT" SHOW_IF="USE_BATCH=='true'" NUM_ROW="14">
			<DEFAULT>10</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_INSERT_OVERWRITE" FIELD="CHECK" NUM_ROW="15" SHOW_IF="(USE_BATCH=='false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ALL_ROWS_FROM_SOURCE_TABLE" FIELD="RADIO" REQUIRED="true" NUM_ROW="16" GROUP="OVERWRITE_SOURCE" SHOW_IF="USE_INSERT_OVERWRITE=='true'">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_A_CUSTOM_QUERY" FIELD="RADIO" REQUIRED="true" NUM_ROW="17" GROUP="OVERWRITE_SOURCE" SHOW_IF="USE_INSERT_OVERWRITE=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SOURCE_TABLE_NAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="18" SHOW_IF="(ALL_ROWS_FROM_SOURCE_TABLE=='true') AND (USE_INSERT_OVERWRITE=='true')">
			<DEFAULT>"mySourceTable"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CUSTOM_QUERY" FIELD="MEMO" REQUIRED="true" NUM_ROW="18" SHOW_IF="(USE_A_CUSTOM_QUERY=='true') AND (USE_INSERT_OVERWRITE=='true')">
			<DEFAULT>"SELECT * FROM source"</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
	</CODEGENERATION>
