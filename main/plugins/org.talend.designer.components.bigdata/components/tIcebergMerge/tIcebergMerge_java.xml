<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20110808A"
    STARTABLE="true"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
	<FAMILY>Big Data/Iceberg</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tIcebergConnection"
      NUM_ROW="1"
    />

    <PARAMETER NAME="SOURCE" FIELD="CLOSED_LIST" NUM_ROW="2">
        <ITEMS DEFAULT="TABLE">
            <ITEM NAME="TABLE" VALUE="TABLE" />
            <ITEM NAME="SELECT_QUERY" VALUE="SELECT_QUERY" />
        </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="SOURCE_TABLE" FIELD="TEXT" NUM_ROW="3" SHOW_IF="SOURCE=='TABLE'">
    	<DEFAULT>"sourceTable"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SOURCE_SELECT_QUERY" FIELD="TEXT" NUM_ROW="3" SHOW_IF="SOURCE=='SELECT_QUERY'">
    	<DEFAULT>"SELECT * FROM sourceTable"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SOURCE_ALIAS" FIELD="TEXT" NUM_ROW="3">
    	<DEFAULT>"S"</DEFAULT>
    </PARAMETER>

	<!-- Target table -->
	<PARAMETER NAME="TARGET_TABLE" FIELD="TEXT" NUM_ROW="4">
		<DEFAULT>"targetTable"</DEFAULT>
	</PARAMETER>

    <PARAMETER NAME="TARGET_ALIAS" FIELD="TEXT" NUM_ROW="4" MAX_LENGHT="4">
    	<DEFAULT>"T"</DEFAULT>
    </PARAMETER>

	<!-- Merge on  -->
	<PARAMETER NAME="MERGE_ON_CONDITION" FIELD="TEXT" NUM_ROW="5">
		<DEFAULT>"sourceTable.id=targetTable.id"</DEFAULT>
	</PARAMETER>

	<!-- When matched update -->
	<PARAMETER NAME="WHEN_MATCHED_UPDATE_CHECK" FIELD="CHECK" NUM_ROW="10"/>
	<PARAMETER NAME="WHEN_MATCHED_UPDATE_CONDITION_CHECK" FIELD="CHECK" SHOW_IF="WHEN_MATCHED_UPDATE_CHECK=='true'" NUM_ROW="10"/>
	<PARAMETER NAME="WHEN_MATCHED_UPDATE_CONDITION" SHOW_IF="(isShow[WHEN_MATCHED_UPDATE_CONDITION_CHECK]) AND (WHEN_MATCHED_UPDATE_CONDITION_CHECK=='true')" FIELD="TEXT" NUM_ROW="11">
		<DEFAULT>"sourceTable.id=1"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="WHEN_MATCHED_UPDATE_CLAUSE" SHOW_IF="WHEN_MATCHED_UPDATE_CHECK=='true')" FIELD="TEXT" NUM_ROW="11">
		<DEFAULT>"name=sourceTable.name, adress=sourceTable.adress"</DEFAULT>
	</PARAMETER>

	<!-- When matched delete -->
	<PARAMETER NAME="WHEN_MATCHED_DELETE_CHECK" FIELD="CHECK" NUM_ROW="12"/>
	<PARAMETER NAME="WHEN_MATCHED_DELETE_CONDITION_CHECK" FIELD="CHECK" SHOW_IF="WHEN_MATCHED_DELETE_CHECK=='true'" NUM_ROW="12"/>
	<PARAMETER NAME="WHEN_MATCHED_DELETE_CONDITION" FIELD="TEXT" SHOW_IF="(isShow[WHEN_MATCHED_DELETE_CONDITION_CHECK]) AND (WHEN_MATCHED_DELETE_CONDITION_CHECK=='true')" NUM_ROW="13">
		<DEFAULT>"targetTable.name='XXX'"</DEFAULT>
	</PARAMETER>

	<!-- When not matched insert -->
	<PARAMETER NAME="WHEN_NOT_MATCHED_INSERT_CHECK" FIELD="CHECK" NUM_ROW="14"/>
	<PARAMETER NAME="WHEN_NOT_MATCHED_INSERT_CONDITION_CHECK" FIELD="CHECK" SHOW_IF="WHEN_NOT_MATCHED_INSERT_CHECK=='true'" NUM_ROW="14"/>
	<PARAMETER NAME="WHEN_NOT_MATCHED_INSERT_CONDITION" FIELD="TEXT" SHOW_IF="(isShow[WHEN_NOT_MATCHED_INSERT_CONDITION_CHECK]) AND (WHEN_NOT_MATCHED_INSERT_CONDITION_CHECK=='true')" NUM_ROW="15">
		<DEFAULT>"sourceTable.name='YYY'"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="WHEN_NOT_MATCHED_INSERT_VALUES" FIELD="TEXT" SHOW_IF="WHEN_NOT_MATCHED_INSERT_CHECK=='true'" NUM_ROW="15">
		<DEFAULT>"sourceTable.id,sourceTable.name"</DEFAULT>
	</PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>
  <RETURNS/>
</COMPONENT>
