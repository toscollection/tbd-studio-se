		<PARAMETER NAME="OUTPUT_TYPE" FIELD="CLOSED_LIST" SHOW="true"
			NUM_ROW="5">
			<ITEMS DEFAULT="STRING">
				<ITEM NAME="STRING" VALUE="STRING" />
				<ITEM NAME="BYTES" VALUE="BYTES" />
				<ITEM NAME="CONSUMER_RECORD" VALUE="CONSUMER_RECORD"
					SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND (KAFKA_VERSION == 'KAFKA_2_2_1' OR KAFKA_VERSION == 'KAFKA_2_4_0' OR KAFKA_VERSION == 'KAFKA_3_2_3'))
						OR (USE_EXISTING_CONNECTION == 'true'
						AND (#LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_2_2_1' OR #LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_2_4_0' OR #LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_3_2_3'))"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5"
			REQUIRED="true">
			<TABLE IF="(OUTPUT_TYPE == 'BYTES') AND (SAVE_OFFSET == 'true')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_byte[]" READONLY="true"
					CUSTOM="true" />
				<COLUMN NAME="offset" TYPE="id_Long" READONLY="true" CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'BYTES') AND (SAVE_OFFSET == 'false')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_byte[]" READONLY="true"
					CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'STRING') AND (SAVE_OFFSET == 'true')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_String" READONLY="true"
					CUSTOM="true" />
				<COLUMN NAME="offset" TYPE="id_Long" READONLY="true" CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'STRING') AND (SAVE_OFFSET == 'false')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_String" READONLY="true"
					CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'CONSUMER_RECORD')
					AND ((USE_EXISTING_CONNECTION == 'false' AND (KAFKA_VERSION == 'KAFKA_2_2_1' OR KAFKA_VERSION == 'KAFKA_2_4_0' OR KAFKA_VERSION == 'KAFKA_3_2_3'))
					OR (USE_EXISTING_CONNECTION == 'true'
					AND (#LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_2_2_1' OR #LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_2_4_0' OR #LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_3_2_3')))"
				READONLY="true">
				<COLUMN NAME="record" TYPE="id_Object" READONLY="true" CUSTOM="true"/>
			</TABLE>
		</PARAMETER>

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tKafkaConnection" NUM_ROW="10"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" />

