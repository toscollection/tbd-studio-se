		</PARAMETER>

		<PARAMETER NAME="USE_BATCH_MAX_DURATION" FIELD="CHECK"
			NUM_ROW="100" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
