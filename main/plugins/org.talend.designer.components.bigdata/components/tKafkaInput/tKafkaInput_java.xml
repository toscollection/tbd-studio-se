			<ITEMS DEFAULT="LARGEST">
				<ITEM NAME="LARGEST" VALUE="LARGEST" />
				<ITEM NAME="SMALLEST" VALUE="SMALLEST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="AUTO_OFFSET_RESET_NEW" FIELD="CLOSED_LIST"
			NUM_ROW="71" GROUP="OFFSET_MANAGEMENT_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND KAFKA_VERSION != 'KAFKA_0_8_2_0') 
                     OR (USE_EXISTING_CONNECTION == 'true' AND (#LINK@NODE.CONNECTION.KAFKA_VERSION != 'KAFKA_0_8_2_0'))">
			<ITEMS DEFAULT="LATEST">
				<ITEM NAME="LATEST" VALUE="LATEST" />
				<ITEM NAME="EARLIEST" VALUE="EARLIEST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="KAFKA_OFFSET_STORAGE" FIELD="CLOSED_LIST"
			NUM_ROW="80" GROUP="OFFSET_MANAGEMENT_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND KAFKA_VERSION == 'KAFKA_0_8_2_0') 
                     OR (USE_EXISTING_CONNECTION == 'true' AND (#LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_0_8_2_0'))">
			<ITEMS DEFAULT="ZOOKEEPER">
				<ITEM NAME="ZOOKEEPER" VALUE="ZOOKEEPER" />
				<ITEM NAME="KAFKA" VALUE="KAFKA" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="KAFKA_DUAL_COMMIT_CHECK" FIELD="CHECK"
			NUM_ROW="80"
			SHOW_IF="(KAFKA_OFFSET_STORAGE == 'KAFKA') 
			         AND 
