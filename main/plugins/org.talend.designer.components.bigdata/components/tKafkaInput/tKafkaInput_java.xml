		<PARAMETER NAME="KAFKA_TOPIC" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40" NB_LINES="3">
			<DEFAULT>"mytopic"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GROUP_ID" FIELD="TEXT" NUM_ROW="50">
			<DEFAULT>"mygroup"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="RESET_OFFSET" FIELD="CHECK" NUM_ROW="60"
			GROUP="OFFSET_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="AUTO_OFFSET_RESET" FIELD="CLOSED_LIST"
